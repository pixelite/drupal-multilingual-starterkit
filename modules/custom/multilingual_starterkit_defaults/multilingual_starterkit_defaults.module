<?php

/**
* Implements hook_views_api().
*/
function multilingual_starterkit_defaults_views_api() {
  return array(
    'api' => 3,
  );
}
/**
  * Implementation of hook_ctools_plugin_api().
  */
function multilingual_starterkit_defaults_ctools_plugin_api($module, $api) {
  if ($module == 'page_manager' && $api == 'pages_default') {
    return array('version' => 1);
  }
}
